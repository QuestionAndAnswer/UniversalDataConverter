{
    "env": {
        "browser": true
    },
    "extends": "eslint:recommended",
	"globals": {
		"define": true,
		"require": true,
		"jQuery": true,
		"QUnit": true
	},
    "rules": {
        "indent": [
			"error",
			"tab",
			{
				"SwitchCase": 1
			}
			],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ]

    }
}
